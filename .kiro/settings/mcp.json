{
  "mcpServers": {
    "chrome-devtools": {
      "command": "npx",
      "args": [
        "-y",
        "chrome-devtools-mcp@latest"
      ],
      "autoApprove": [
        "list_console_messages",
        "evaluate_script",
        "list_network_requests",
        "take_snapshot",
        "wait_for",
        "get_network_request",
        "navigate_page",
        "take_screenshot",
        "list_pages"
      ]
    }
  }
}